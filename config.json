{
  "browser": {
    "user_agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36",
    "default_url": "https://www.google.com",
    "enable_javascript": true,
    "enable_cookies": true,
    "enable_images": true,
    "timeout": 30000
  },
  "login": {
    "auto_detect_forms": true,
    "common_selectors": {
      "username": [
        "input[name='username']",
        "input[name='email']",
        "input[name='user']",
        "input[name='login']",
        "input[type='email']",
        "input[id*='username']",
        "input[id*='email']",
        "input[id*='user']",
        "input[id*='login']"
      ],
      "password": [
        "input[name='password']",
        "input[name='pass']",
        "input[name='pwd']",
        "input[type='password']",
        "input[id*='password']",
        "input[id*='pass']",
        "input[id*='pwd']"
      ],
      "submit": [
        "input[type='submit']",
        "button[type='submit']",
        "button:contains('Login')",
        "button:contains('Sign In')",
        "button:contains('Log In')",
        "input[value*='Login']",
        "input[value*='Sign In']",
        "input[value*='Log In']"
      ]
    }
  },
  "extraction": {
    "remove_scripts": true,
    "remove_styles": true,
    "remove_navigation": true,
    "remove_headers": true,
    "remove_footers": true,
    "min_text_length": 10,
    "max_text_length": 1000000,
    "preserve_formatting": false,
    "extract_links": false,
    "extract_images": false
  },
  "auto_mode": {
    "enabled": false,
    "delay_between_pages": 3,
    "max_pages_per_session": 100,
    "follow_redirects": true,
    "respect_robots_txt": true,
    "navigation_patterns": [
      "next_page",
      "pagination",
      "article_list"
    ]
  },
  "output": {
    "default_format": "txt",
    "auto_save": false,
    "save_after_each_page": false,
    "filename_template": "extracted_text_{timestamp}",
    "include_metadata": true,
    "include_url": true,
    "include_title": true,
    "include_timestamp": true
  },
  "ui": {
    "window_size": {
      "width": 1400,
      "height": 900
    },
    "panel_widths": {
      "left_panel": 400,
      "browser_min_height": 500,
      "text_display_max_height": 200
    },
    "theme": "default",
    "font_size": 12,
    "show_status_bar": true,
    "show_progress_bar": true
  },
  "advanced": {
    "debug_mode": false,
    "log_level": "INFO",
    "save_logs": false,
    "log_file": "web_explorer.log",
    "enable_console_output": true,
    "custom_scripts": [],
    "custom_styles": []
  }
} 